<?xml version='1.0' encoding='utf-8'?>
<widget id="net.line72.montclair" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Montclair</name>
    <description>
        Birmingham, AL Real Time Bus Tracker.
    </description>
    <author email="line72@line72.net" href="http://line72.net">
        Marcus Dillavou
    </author>
    <content src="https://montclair.line72.net/index.html" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation hap-rule="yes" href="https://montclair.line72.net/*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Fullscreen" value="false" />
</widget>
